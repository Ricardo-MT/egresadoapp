{
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "^https://firebasestorage.googleapis.com",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Access-Control-Allow-Methods",
            "value": "GET, POST, PUT, DELETE, OPTIONS, HEAD"
          },
          {
            "key": "Content-Type",
            "value": "application/json"
          },
          {
            "key": "Content-Type",
            "value": "image/png"
          },
          {
            "key": "Content-Type",
            "value": "image/jpg"
          },
          {
            "key": "Content-Type",
            "value": "image/jpeg"
          }
        ]
      },
      {
        "source": "^http://localhost:8000",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Access-Control-Allow-Methods",
            "value": "GET, POST, PUT, DELETE, OPTIONS, HEAD"
          },
          {
            "key": "Content-Type",
            "value": "application/json"
          },
          {
            "key": "Content-Type",
            "value": "image/png"
          },
          {
            "key": "Content-Type",
            "value": "image/jpg"
          },
          {
            "key": "Content-Type",
            "value": "image/jpeg"
          }
        ]
      },
      {
        "source": "^https://egresadoapp.web.app",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Access-Control-Allow-Methods",
            "value": "GET, POST, PUT, DELETE, OPTIONS, HEAD"
          },
          {
            "key": "Content-Type",
            "value": "application/json"
          },
          {
            "key": "Content-Type",
            "value": "image/png"
          },
          {
            "key": "Content-Type",
            "value": "image/jpg"
          },
          {
            "key": "Content-Type",
            "value": "image/jpeg"
          }
        ]
      },
      {
        "source": "^https://egresadoapp.firebaseapp.com",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Access-Control-Allow-Methods",
            "value": "GET, POST, PUT, DELETE, OPTIONS, HEAD"
          },
          {
            "key": "Content-Type",
            "value": "application/json"
          },
          {
            "key": "Content-Type",
            "value": "image/png"
          },
          {
            "key": "Content-Type",
            "value": "image/jpg"
          },
          {
            "key": "Content-Type",
            "value": "image/jpeg"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}